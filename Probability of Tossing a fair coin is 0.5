noe=zeros(1,500); //Array to store no. of Iteration
no=zeros(1,500);  //Array to store no. of probability to getting Head
head=0;            //variable to store No of time getting Head
m=1;
for n=10:10:5000
    //Averaging the Performance of Program
//    for avg=1:10
        num=round(rand(1,n)*100);
        no(m)=n;
        for i=1:n
            if pmodulo(num(i),2) == 0
                head=head+1;
            end
        end
//    end
//    head=head/10;
    noe(m)=head/n;
    head=0;
    m=m+1;
end
plot(no,noe,'r');
xlabel('No. of time coin flips to toss','fontsize','2');
ylabel('Probability of Head','fontsize','2');
legend("Probability of Head");
title('Average Case Analysis of Coin Tossing','fontsize','3','color','blue');
xgrid(2)




